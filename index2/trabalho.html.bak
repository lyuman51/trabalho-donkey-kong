
 <!DOCTYPE html>
 <html>
  <head>

   <meta charge="UTF=8"/>
   <meta name="viewport" content="width=device-initial scale1.0"/>
   <link rel="stylesheet" type="text/css" href="css.estilo.css"/>
   <title>trabalho</title>
  </head>
  
  <body>
   <center><canvas id = "canvas" width="1900px" height="900px"></canvas></center>
  </body>
  
  <script>
  
  var c=document.getElementById("canvas");
  var ctx=c.getContext("2d");
 
  // colunas // 
  ctx.strokeStyle = "red"; 
  ctx.moveTo(650,800);
  ctx.lineTo(650,350);
  ctx.stroke();
  
  ctx.moveTo(700,800);
  ctx.lineTo(700,350);
  ctx.stroke();  
  
  ctx.moveTo(1250,800);
  ctx.lineTo(1250,350);
  ctx.stroke();
  
  ctx.moveTo(1300,800);
  ctx.lineTo(1300,350);
  ctx.stroke();
  
  ctx.moveTo(1300,800);
  ctx.lineTo(1300,350);
  ctx.stroke();
  
  // parte de cima //
  
  ctx.moveTo(450,800);
  ctx.lineTo(1500,800);
  ctx.stroke();
  
  
  
  
  
  
  
  </script>
  
  
 </html>